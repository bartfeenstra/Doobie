    "license": "MIT",
    "homepage": "http://github.com/bartfeenstra/Doobie",
    "type": "library",
    "authors": [
        {
            "name": "Bart Feenstra",
            "email": "bart@mynameisbart.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": "0.6.*"
    },
    "autoload": {
        "psr-4": {
            "BartFeenstra\\Doobie\\": "src",
            "BartFeenstra\\Doobie\\Tests\\": "tests/src"
        }
    }
}
